{
    "Log to the console": {
        "prefix": "clg",
        "body": ["console.log($1);", "$0"],
        "description": "Log to the console"
    },
    "Try-catch block": {
        "prefix": "try",
        "body": [
            "try {",
            "    $1",
            "} catch (error) {",
            "   console.error(error);",
            "}"
        ],
        "description": "Try-catch block"
    }
}
